{
  "name": "@playkit-js/playkit-js-ui",
  "version": "0.77.13",
  "description": "",
  "keywords": [
    "kaltura",
    "player",
    "playkit-js",
    "playkit-js-ui",
    "html5 player"
  ],
  "homepage": "https://github.com/kaltura/playkit-js-ui#readme",
  "bugs": {
    "url": "https://github.com/kaltura/playkit-js-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaltura/playkit-js-ui.git"
  },
  "license": "AGPL-3.0",
  "main": "dist/playkit-ui.js",
  "typings": "dist/playkit-js-ui.d.ts",
  "sass": "src/styles/exported.scss",
  "files": [
    "dist/**/*",
    "src/**/*",
    "translations/**/*",
    "flow-typed/**/*"
  ],
  "scripts": {
    "serve": "webpack serve --open --mode development",
    "watch": "webpack --watch --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build": "yarn run build:prod && yarn run build:types",
    "build:types": "tsc --build tsconfig-lib.json && mkdir -p lib && api-extractor run --local",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn run type-check --watch",
    "lint": "eslint src/ --ext .js --ext .ts",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --write .",
    "test": "karma start karma.conf.js",
    "test:debug": "DEBUG_UNIT_TESTS=1 karma start karma.conf.js --auto-watch --no-single-run --browsers Chrome",
    "test:watch": "karma start karma.conf.js --auto-watch --no-single-run",
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "precommit": "npm run build:prod && npm run type-check && npm run lint:fix",
    "pushTaggedRelease": "git push --follow-tags --no-verify origin master",
    "release": "standard-version"
  },
  "dependencies": {
    "preact": "10.4.6",
    "preact-i18n": "2.0.0-preactx.2",
    "react-redux": "7.2.1",
    "redux": "4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.23.3",
    "@microsoft/api-extractor": "^7.38.0",
    "@playkit-js/browserslist-config": "^1.0.7",
    "@playkit-js/kaltura-player-js": "latest",
    "@playkit-js/playkit-js": "latest",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^9.1.1",
    "@types/preact-i18n": "1.2.0",
    "@types/sinon": "^10.0.20",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-loader": "^9.1.3",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "3.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mocha": "^10.2.0",
    "eslint-plugin-prettier": "^5.1.2",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.0.0",
    "prettier": "^3.1.1",
    "sass": "^1.58.0",
    "sass-loader": "^9.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "standard-version": "^6.0.1",
    "style-loader": "^1.2.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "extends @playkit-js/browserslist-config"
  ],
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "kcc": {
    "name": "playkit-ui"
  }
}
